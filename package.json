{
  "name": "@deltares/fews-web-oc-components",
  "version": "0.1.3-alpha.5",
  "description": "Library for components used in the FEWS web oc",
  "license": "AGPL",
  "type": "module",
  "main": "./dist/fews-web-oc-components.umd.js",
  "typings": "./dist/types/src/index.d.ts",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress && npm pack",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:unit": "jest unit",
    "test:e2e": "vue-cli-service test:e2e",
    "ci:test:e2e": "vue-cli-service test:e2e --headless",
    "ci:storybook:serve": "vue-cli-service storybook:serve -p 6006 -s public -- --ci --quiet",
    "ci:start-and-test-storybook": "cross-env START_SERVER_AND_TEST_INSECURE=1 \"start-server-and-test ci:storybook:serve http://localhost:6006 ci:test:e2e\"",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006",
    "sonar": "sonar-scanner -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN -Dsonar.projectKey=$SONAR_KEY -Dsonar.projectName='Delft-FEWS Web OC Components'"
  },
  "dependencies": {
    "@cypress/vue": "^2.2.3",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "shx": "^0.3.3",
    "sonarqube-scanner": "^3.0.1",
    "vue": "2.7",
    "vue-class-component": "^7.2.6",
    "vue-keypress": "^2.1.2",
    "vue-property-decorator": "^9.1.2",
    "vuetify": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@mdi/font": "^6.3.95",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/vue": "^6.0.26",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.176",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-e2e-cypress": "^4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.14",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.3.5",
    "babel-loader": "^8.0.4",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "^6.5.3",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^13.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "jsdom": "^21.1.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^8.4.0",
    "typescript": "^4.8.2",
    "vue-cli-plugin-storybook": "~2.1.0",
    "vue-cli-plugin-vuetify": "~2.4.3",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "jest-junit": {
    "outputDirectory": "reports/",
    "outputName": "junit.xml"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deltares/fews-web-oc-components"
  },
  "bugs": {
    "url": "https://github.com/Deltares/fews-pi-requests/issues"
  }
}
