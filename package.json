{
  "name": "@deltares/fews-web-oc-components",
  "version": "0.1.3-alpha.6",
  "description": "Library for components used in the FEWS web oc",
  "license": "AGPL",
  "type": "module",
  "main": "./dist/fews-web-oc-components.umd.js",
  "typings": "./dist/types/src/index.d.ts",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress && npm pack",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:unit": "jest unit",
    "test:e2e": "vue-cli-service test:e2e",
    "ci:test:e2e": "vue-cli-service test:e2e --headless",
    "ci:storybook:serve": "vue-cli-service storybook:serve -p 6006 -s public -- --ci --quiet",
    "ci:start-and-test-storybook": "cross-env START_SERVER_AND_TEST_INSECURE=1 \"start-server-and-test ci:storybook:serve http://localhost:6006 ci:test:e2e\"",
    "lint": "vue-cli-service lint",
    "storybook:build": "vue-cli-service storybook:build",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006",
    "sonar": "sonar-scanner -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN -Dsonar.projectKey=$SONAR_KEY -Dsonar.projectName='Delft-FEWS Web OC Components'"
  },
  "dependencies": {
    "vue": "2.7",
    "vue-class-component": "^7.2.6",
    "vue-keypress": "^2.1.2",
    "vue-property-decorator": "^9.1.2",
    "vuetify": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@mdi/font": "^7.2.96",
    "@storybook/addon-docs": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/vue": "^7.3.2",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^20.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^1.3.6",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "eslint": "^8.47.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-junit": "^16.0.0",
    "jest-teamcity-reporter": "^0.9.0",
    "jsdom": "^22.1.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "shx": "^0.3.3",
    "sonarqube-scanner": "^3.0.1",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "vue-cli-plugin-storybook": "^2.1.0",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.0.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.4"
  },
  "jest-junit": {
    "outputDirectory": "reports/",
    "outputName": "junit.xml"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deltares/fews-web-oc-components"
  },
  "bugs": {
    "url": "https://github.com/Deltares/fews-web-oc-components/issues"
  }
}
